<properties>
<comment></comment>

<!-- ACCESS TO SEMP GATEWAY -->
<entry key="HOST_OS">auto</entry> <!-- auto possible -->
<entry key="HTTP_ADDRESS">auto</entry> <!-- auto possible -->
<entry key="HTTP_PORT">8080</entry>
<entry key="HTTP_BASE_URL">/semp</entry>

<!-- ACCESS TO STATUS AND CONTROL WEBSITE -->
<entry key="WEBSITE_MODE">expert</entry> <!-- readonly: Page in readonly mode // fullaccess: Page in full access mode // expert: Page in full access and debug mode  -->
<entry key="WEBSITE_URL">/status</entry> <!-- none: no Website url: url of Website e.g. /status -->
<entry key="WEBSITE_REFRESH">10</entry> <!-- 0: Page refresh disabled // 1..x: Page refreh in seconds. -->
<entry key="WEBSITE_LOGO">logo.png</entry>
<entry key="WEBSITE_FAVICON">logo.png</entry>
<entry key="WEBSITE_CSS">semp.css</entry>

<!-- SIMPLE ENERGY MANAGEMENT PROTOCOL -->
<entry key="SEMP_GW_DEVICE_ID">1-00D43D7E-812233445566-0</entry> <!-- auto:  SEMP_GW_DEVICE_ID = SSDP_UUID -->
<entry key="SEMP_GW_FRIENDLY_NAME">JavaGateway</entry>
<entry key="SEMP_GW_MANUFACTORER">SANDHOLZ ENGINEERING</entry>
<entry key="SEMP_XSD_SCHEMA">SEMP-1.1.5-public_release.xsd</entry> <!-- none: disable XSD-Schema Validation (SEMP-1.1.5-public_release.xsd) -->

<!-- SIMPLE SERVICE DISCOVERY PROTOCOL -->
<entry key="SSDP_PRODUCT_NAME">SomeGatewaySSDPServer</entry>
<entry key="SSDP_PRODUCT_VERSION">1.0.1</entry>
<entry key="SSDP_LOCATION_PATH">/semp/description.xml</entry>
<entry key="SSDP_UUID">auto</entry> <!-- auto possible -->
<entry key="SSDP_MULTICAST_ADDRESS">239.255.255.250:1900</entry>
<entry key="SSDP_UNICAST_ADDRESS">239.255.255.250:1900</entry>
<entry key="SSDP_CACHE_CONTROL">1800</entry>
<entry key="SSDP_NT_ROOT_DEVICE">upnp:rootdevice</entry>
<entry key="SSDP_NT_DEVICE_TYPE">urn:schemas-simple-energy-management-protocol:device:Gateway:1</entry>

</properties>
